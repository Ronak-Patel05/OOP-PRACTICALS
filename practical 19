public class College 
{
	String collegeName;

	College(String name)
	{
		collegeName = name;
	}
	class Admission
	{
		String studentName;
		String course;
		
		void acceptDetails(String name, String c)
		{
			studentName = name;
			course = c;
		}
		void displayDetails()
		{
			System.out.println("College Name:" + collegeName);
			System.out.println("Student Name:" + studentName);
			System.out.println("CourseName:" + course);
		}
	}
	public static void main(String[] args)
	{
		College c = new College("Saffrony Institute of Engineering GTU");
		College.Admission a = c.new Admission();
		
		a.acceptDetails("Ronak", "Computer Engineering");
		a.displayDetails();
	}
}
